				,
				{10,5,2, true}
				,
				{11,1,5, false}
				,
				{30,10,5, true}
				,
				{11,2,1, false}
				,
				{20,10,2, true}
				,
				{25,2,5, false}
				,
				{20,5,0, true}
				,
				{10,5,0, false}
				,
				{30,5,0, true}
				,
				{10,1,0, false}
				,
				{5,2,0, false}
				,
				{5,2,0, true}
				,
				{5,2,0, false}
				,
				{5,2,0, true}
				
			};
			
			
			
			
			void memoryUp(int f){
			memory+=Info[f].memory;
			}
			
			
			void memoryDown(int f){
			memory-=Info[f].memory;
			}
			
			
			void setInstructions(int i){
			instructionsNumber[i]=Info[i].instructions;
			}
			
			
